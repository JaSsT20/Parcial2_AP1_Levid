@page "/C_Empaquetado"

<table class='table'>
    <thead>
        <th>Id</th>
        <th>Descripcion</th>
        <th>Cantidad</th>
        <th></th>
    </thead>
    <tbody>
        @foreach(var det in empacado.EmpacadoDetalles)
        {
            <tr>
                <td>@det.</td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
            </tr>
        }
    </tbody>
</table>

@code{
    public Empacado empacado { get; set; } = new Empacado();
}